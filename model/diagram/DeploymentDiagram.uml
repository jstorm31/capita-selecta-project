@startuml
cloud Cloud {
    node FrontendServer {
        component LoadBalancer
        component Autoscaler
        component Endpoint
    }
    node VirtualMachine {
        component Worker
        component Service
        database Database
    }
    FrontendServer -(0- VirtualMachine
}

cloud Vendor_1 {
    node Server_1 {
        component PaymentProvider_1
    }
}

cloud Vendor_2 {
    node Server_2 {
        component PaymentProvider_2
    }
}

cloud Vendor_3 {
    node Server_3 {
        component PaymentProvider_3
    }
}

frame Clients {
    frame cl_1 [
        Client 1
    ]
    frame cl_2 [
        Client 2
    ]
    frame cl_n [
        Client N
    ]
}

Clients -(0- FrontendServer
FrontendServer -(0- PaymentProvider_1
FrontendServer -(0- PaymentProvider_2
FrontendServer -(0- PaymentProvider_3
@enduml