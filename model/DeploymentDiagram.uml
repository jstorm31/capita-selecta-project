@startuml
cloud Cloud {
    node FrontendServer {
        component LoadBalancer
        component Autoscaler
        component Endpoint
    }
    node VirtualMachine {
        component Worker
        component Service
        database Database
    }
    FrontendServer -(0- VirtualMachine
}

frame Clients {
    frame cl_1 [
        Client 1
    ]
    frame cl_2 [
        Client 2
    ]
    frame cl_n [
        Client N
    ]
}

Clients -(0- FrontendServer
@enduml